<form id="add_entry" class=add-entry>
  <dl>
    <dt>Title:
      <dd><input type="text" size=30 name="title">
    <dt>Text:
      <dd><textarea name="text" rows=5 cols=40></textarea>
      <dd><input type="submit" value="Share">
  </dl>
</form>

<ul class=entries>
  {{ #entries }}
  <li>
      <h2> <a href="?id={{ id }}"> {{ title }} </a> </h2>

      {{ text }}
  
  {{ /entries }}
</ul>